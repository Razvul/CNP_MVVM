<Window x:Class="CNP_MVVM.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CNP_MVVM"
        mc:Ignorable="d"
        Title="MainWindow"
        Height="450"
        Width="800">

    <Grid >
        <StackPanel>
            <Button x:Name="Add_Person_Test"
              Width="100"
              Height="30"
              Content="Add Person"
              Click="Add_Person_Test_Click">
            </Button>

            <Button x:Name="CNP_Test"
              Width="100"
              Height="30"
              Content="CNP"
              Click="CNP_Test_Click">
            </Button>
            <Button x:Name="Data"
              Width="100"
              Height="30"
              Content="Data"
              Click="Data_Click">
            </Button>
        </StackPanel>
    </Grid>
</Window>